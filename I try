#include <stdio.h>
#include <stdlib.h>
int main()
{
	int a, b, n;
	int sum = 0;
	printf("enter range\n");
	scanf_s("%d%d", &a, &b);
	if (a > b) {
		printf("error, enter again\n");
	}
	printf("enter number\n");
	scanf_s("%d", &n);
	while (n != 0) {
		printf("enter number\n");
		scanf_s("%d", &n);
		if (n > a & n < b) {
			sum = sum + n;
		}
	}
	printf("%d\sum\n", sum);
	system("pause");
	return 0;
}

#include <stdio.h>
#include <stdlib.h>
int main()
{
	int a, b, c, d;
	double x = 0;
	scanf_s("%d%d%d%d", &a, &b, &c, &d);
	if (a < 0 || b < 0 || c < 0 || d < 0) {
		printf("error, enter again\n");
	}
	while (b == 0 || d == 0) {
	  printf("error, enter again\n");
	  scanf_s("%d%d%d%d", &a, &b, &c, &d);
	}
		 x = a / b + c / d;
	printf("%.1lf\n", x);
	system("pause"); 
	return 0;
}

#include <iostream>
using namespace std;
int main() {
  int N, K;
    cin >> N >> K;
    cout << K / N;
  return 0;
}
#include <iostream>
using namespace std;
int main() {
 int N, K;
    cin >> N >> K;
    cout << K-(K/N)*N;
  return 0;
}
#include <iostream>
using namespace std;
int main() {
  int N;
    cin >> N;
    cout << N / 100 + N % 100 / 10 + N % 10;
  return 0;
}
#include <iostream>
using namespace std;
int main() {
  int u1, u2, u3, p;
    cin >> u1 >> u2 >> u3;
   p = u1/2 + u2/2 + u3/2 + u1%2 + u2%2 + u3%2;
   cout << p;
  return 0;
}
